<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.api_test.dao.MessageRepository">
    <insert id="save" parameterType="com.example.api_test.domain.Message">
        insert into
            message (
                cmn,
                payload
        ) values (
                #{cmn},
                #{payload}
        )
    </insert>
</mapper>